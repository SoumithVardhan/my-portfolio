/* Emergency CSS Reset to Fix Visibility Issues */\n\n/* Ensure all text is visible with proper contrast */\n* {\n  color: inherit !important;\n}\n\n/* Force proper background and text colors */\nhtml {\n  background-color: hsl(0, 0%, 4%) !important;\n  color: hsl(0, 0%, 99%) !important;\n}\n\nbody {\n  background-color: hsl(0, 0%, 4%) !important;\n  color: hsl(0, 0%, 99%) !important;\n}\n\n/* Ensure main app container has proper colors */\n.app {\n  background-color: hsl(0, 0%, 4%) !important;\n  color: hsl(0, 0%, 99%) !important;\n}\n\n/* Fix text visibility in all sections */\np, h1, h2, h3, h4, h5, h6, span, div {\n  color: hsl(0, 0%, 99%) !important;\n  opacity: 1 !important;\n}\n\n/* Fix grey text */\n.section-desc,\n.location p,\n.sub-title p,\n.left-socials,\n.grey-color {\n  color: hsl(0, 0%, 85%) !important;\n  opacity: 1 !important;\n}\n\n/* Ensure primary color is blue */\n.role-text,\n.primary-color {\n  color: hsl(210, 100%, 60%) !important;\n}\n\n/* Fix button colors */\n.primary-button {\n  background-color: hsl(210, 100%, 60%) !important;\n  color: hsl(0, 0%, 4%) !important;\n  border: 1px solid hsl(210, 100%, 60%) !important;\n}\n\n.secondary-button {\n  border: 1px solid hsl(210, 100%, 60%) !important;\n  color: hsl(0, 0%, 99%) !important;\n}\n\n.secondary-button:hover {\n  background-color: hsl(210, 100%, 60%) !important;\n  color: hsl(0, 0%, 4%) !important;\n}\n